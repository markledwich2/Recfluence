{
    "docker.defaultRegistryPath": "ytnetworks.azurecr.io",
    "azureFunctions.deploySubpath": "YtFunctions/bin/Release/net6.0/publish",
    "azureFunctions.projectLanguage": "C#",
    "azureFunctions.projectRuntime": "~3",
    "debug.internalConsoleOptions": "neverOpen",
    "azureFunctions.preDeployTask": "publish",
    "cSpell.words": [
        "Jsonl",
        "Mutuo",
        "dataform",
        "josnl"
    ],
    "files.exclude": {
        "**/bin": true,
        "**/obj": true,
        "**/.data": true
    },
    "workbench.colorCustomizations": {
        "[Default Dark+]": {
            "activityBar.background": "#302230",
            "statusBar.background": "#974597"
        }
    },
    "json.schemas": [
        {
            "fileMatch": [
                "*.appcfg.json"
            ],
            "url": "./YtReader/AppCfg.schema.json"
        },
        {
            "fileMatch": [
                "*.rootcfg.json"
            ],
            "url": "./YtReader/RootCfg.schema.json"
        }
    ],
}