{
    "compilerOptions": {
        "outDir": "./dist/",
        "sourceMap": true,
        "noImplicitAny": true,
        "module": "commonjs",
        "target": "es6",
        "jsx": "react",
        "allowSyntheticDefaultImports": true,
        "keyofStringsOnly": true
    },
    "include": [
        "./src/**/*",
    ],
    "typeRoots": [
        "./src/types",
        "node_modules/@types"
    ],
    "types": [
        "youtube"
    ]
}